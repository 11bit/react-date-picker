{
  "extends": "airbnb",
  "rules": {
    "semi": 0,
    "comma-dangle": 0,
    "react/wrap-multilines": 0,
    "react/sort-comp": 0,
    "eqeqeq": 0,
    "prefer-template": 1,
    "no-console": 0,
    "no-loop-func": 0,
    "func-names": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "react/no-did-mount-set-state": 0,
    "react/jsx-no-bind": 0,
    "react/prop-types": [0, { "ignore": [], "customValidators": [] }]
  }
}
